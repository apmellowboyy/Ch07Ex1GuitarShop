@{
    ViewData["Title"] = "ContactUs";
}

<h1>Contact Us</h1>
<p>Here is how you can reach us:</p>

<table>
    <thead>
        <tr>
            <th>Contact Method</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Key</td>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>
</table>